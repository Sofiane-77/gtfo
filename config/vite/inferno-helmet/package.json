{
  "name": "inferno-helmet",
  "version": "0.1.0",
  "private": true,
  "description": "Thread-safe Helmet for Inferno",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "build": "rimraf build && npm run build:types && npm run build:common && npm run build:module",
    "build:common": "rimraf build/common && swc src -d build/common --source-maps --config-file ./.swcrc.common.json",
    "build:module": "rimraf build/module && swc src -d build/module --source-maps --config-file ./.swcrc.module.json",
    "build:types": "rimraf build/types && tsc --project tsconfig.types.json",
    "lint": "eslint --cache",
    "typecheck": "tsc"
  },
  "keywords": [
    "helmet",
    "inferno",
    "meta"
  ],
  "license": "Apache-2.0",
  "dependencies": {},
  "peerDependencies": {
    "inferno": "^8.0.0",
    "inferno-create-element": "^8.0.0"
  },
  "devDependencies": {
    "@dr.pogodin/eslint-configs": "^0.2.7",
    "@swc/cli": "^0.8.0",
    "@swc/core": "^1.15.11",
    "@tsconfig/recommended": "^1.0.13",
    "inferno": "^9.0.11",
    "inferno-create-element": "^9.0.11",
    "rimraf": "6.1.2",
    "typescript": "^5.9.3"
  }
}
